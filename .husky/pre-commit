# ── BEGIN nvm bootstrap ───────────────────────────────────────────────────────
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"      # loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"
# ── END nvm bootstrap ─────────────────────────────────────────────────────────

pnpm lint-staged
